package DragDropD5;

{$R *.res}
{$R 'DropSource.dcr'}
{$R 'DropTarget.dcr'}
{$R 'DropBMPSource.dcr'}
{$R 'DropBMPTarget.dcr'}
{$R 'DropURLSource.dcr'}
{$R 'DropURLTarget.dcr'}
{$R 'DropPIDLSource.dcr'}
